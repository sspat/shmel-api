{
    "name": "sspat/shmel-api",
    "description": "API facade for Shmel-pereezd API",
    "type": "library",
    "version": "1.1.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrik Foldes",
            "email": "studio22@mail.ru"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "lib-libxml": "*",
        "ext-soap": "*",
        "webmozart/assert": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "squizlabs/php_codesniffer": "2.8.1"
    },
    "autoload": {
        "psr-4": {
            "sspat\\ShmelAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "sspat\\ShmelAPI\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 1800,
        "preferred-install": "dist"
    }
}
